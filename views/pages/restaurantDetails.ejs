    <!-- Map Begin -->
    <div class="map">
        <div class="border border-info">
        <iframe
            src="<%= fData.Embedded_Map_Link %>"
            height="500" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
        <div class="map-inside">
            <i class="icon_pin"></i>
            <div class="inside-widget">
                <h4><%= fData.Restaurant_Name %></h4>
                <ul>
                    <li>Phone: <%= fData.Restaurant_PhoneNo %></li>
                    <li><%= fData.Restaurant_Address %></li>
                </ul>
            </div>
        </div>
        </div>
    </div>
    <!-- Map End -->

    <!-- Restaurant Details Section Begin -->
    <section class="product-details spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6">
                    <div class="product__details__pic">
                        <div class="product__details__pic__item">
                            <img class="rounded-lg"
                                src="/img/restaurants/Restaurant.png" alt="image">
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="product__details__text">
                        <h3><%= fData.Restaurant_Name %></h3>
                        <ul>
                            <li><b>Owner Name</b> <span><%= fData.Owner_Name %></span></li>
                            <li><b>Prefered Restaurant Type</b> <span><%= fData.Prefered_Restaurant_Type %></span></li>
                            <li><b>Email</b> <span><%= fData.Restaurant_Email %></span></li>
                            <% if(req.session.loggedRestaurant==true){ %>
                                <li><b>Password</b> <span><%= fData.Restaurant_Password %></span></li>
                            <% } %>                            
                            <li><b>Phone No.</b> <span><%= fData.Restaurant_PhoneNo %></span></li>
                            <li><b>Open Time</b> <span><%= fData.Open_Time %></span></li>
                            <li><b>Certified By</b> <span><%= fData.Certified_By %></span></li>
                            <li><b>Certification Licence No.</b> <span><%= fData.Certitied_Licence_No %></span></li>
                            <li><b>GST No.</b> <span><%= fData.GSTNo %></span></li>
                            <li><b>Address</b> <span><%= fData.Restaurant_Address %></span></li>
                            <% if(req.session.loggedRestaurant==true){ %>
                                <li><b>Embedded Map Link</b> <span><%= fData.Embedded_Map_Link %></span></li>
                            <% } %>
                            <li><b>Be in Touch on</b>
                                <div class="share">
                                    <a href="<%= fData.Facebook %>" target="_blank"><i class="fa fa-facebook"></i></a>
                                    <a href="<%= fData.Twitter %>" target="_blank"><i class="fa fa-twitter"></i></a>
                                    <a href="<%= fData.LinkedIn %>" target="_blank"><i class="fa fa-linkedin"></i></a>
                                    <a href="<%= fData.Instagram %>" target="_blank"><i class="fa fa-instagram"></i></a>
                                    <!-- Example For Making Sharable Links
                                        <a href="whatsapp://send?text=https://sailsjs.com/documentation/concepts" data-action="share/whatsapp/share"><i class="fa fa-whatsapp"></i></a>
                                        <a href="#"><iframe src="https://www.facebook.com/plugins/share_button.php?href=https%3A%2F%2Fdevelopers.facebook.com%2Fdocs%2Fplugins%2F&layout=button_count&size=small&width=96&height=20&appId" width="96" height="20" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allow="encrypted-media"></iframe></a> 
                                    -->
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Restaurant Details Section End -->
    <!-- Items -->
    <section class="related-product">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title related__product__title">
                        <h2>Foods Offered</h2>
                    </div>
                    <center>
                    <form action="/pages/availableItems/<%= fData.Restaurant_Name %>" method="post">
                        <button type="submit" class="btn btn-primary btn-lg btn-block">Items</button>
                        <input type="hidden" name="_csrf" value="<%=_csrf%>">
                    </form>
                </center>
                </div>
            </div>
        </div>
    </section>