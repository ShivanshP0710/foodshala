<br>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <center><h1><u>Customer Login</u></h1></center>
            <br>
            <form action="/cLoginCheck" method="post" class="form_login" name="customer_Login">
                <div class="form-group">
                    <% if(flash && flash.err) { %>
                      <% if(flash.err=="Invalid Email Id / Password") { %>
                        <p class="alert alert-danger">
                          <% Object.keys(flash.err).forEach(function(error){ %>
                            <%= flash.err[error] %>
                          <% }) %>
                        </p>
                      <% } %>
                      </p>                      
                <!--                       
                      <% if(flash.err=="Session Timed Out") { %>
                        <p class="alert alert-danger">
                          <% Object.keys(flash.err).forEach(function(error){ %>
                            <%= flash.err[error] %>
                          <% }) %>
                        </p>
                      <% } %> -->
                    <% } %>
                  </div>
                <br>
                <div class="form-group">
                    <label>Email address</label>
                    <input type="email" class="form-control" name="eLEmail" placeholder="Enter email" >
                </div>
                <div class="form-group">
                    <label>Password</label>
                    <input type="password" class="form-control" name="eLPassword" placeholder="Enter Password" >
                </div>
                <button type="submit" class="btn btn-primary btn-lg btn-block">Login</button>
                <input type="hidden" name="_csrf" value="<%=_csrf%>">
            </form>
        </div>
    </div>
</div>