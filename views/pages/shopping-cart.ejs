    <!-- Shoping Cart Section Begin -->
    <section class="shoping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title related__product__title">
                        <h2>Foods In Cart</h2>
                    </div>

                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Customer Name</th>
                                <th>Restaurant Name</th>
                                <th>Item Name</th>
                                <th>Price</th>
                                <!-- <th>Quantity</th> -->
                                <th> </th>
                            </tr>
                        </thead>
                        <tbody>
                            <!-- ejs syntax -->
                            <% var Customer =  "" %>
                            <% fCData.forEach(function(dbCData){ %>
                            <tr>
                                <% Customer =  dbCData.Customer_Name %>
                                <td><%= dbCData.Customer_Name %></td>
                                <td><%= dbCData.Restaurant_Name %></td>
                                <td><%= dbCData.Item_Name %></td>
                                <td>Rs.<%= dbCData.Price %></td>
                                <!-- <td class="shoping__cart__quantity">
                                    <div class="quantity">
                                        <div class="pro-qty">
                                            <input type="text" value="1">
                                        </div>
                                    </div>
                                </td> -->
                                <td>
                                    <form action="/iDelete/<%= dbCData.id %>" method="post">
                                        <button type="submit"  class="btn btn-danger">Remove</button>
                                        <input type="hidden" name="_csrf" value="<%=_csrf%>">
                                    </form>
                                </td>
                            </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="shoping__cart__btns">
                        <a href="/" class="primary-btn cart-btn cart-btn-right"><span class="fa fa-arrow-right"></span> CONTINUE SHOPPING</a>
                    </div>
                </div>
                <div class="col-lg-12">
                    <div class="shoping__checkout">
                        <!-- <h5>Cart Total</h5>
                        <ul>
                            <li>Total Amount<span>Rs 130</span></li>
                        </ul> -->
                        <form action="/pages/checkOut/<%= Customer %>" method="post">
                            <button type="submit"  class="btn primary-btn btn-lg btn-block">PROCEED TO CHECKOUT</button>
                            <input type="hidden" name="_csrf" value="<%=_csrf%>">
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Shoping Cart Section End -->