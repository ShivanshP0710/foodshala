    <!-- Shoping Cart Section Begin -->
    <section class="shoping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title related__product__title">
                        <h2>Foods In Cart</h2>
                    </div>

                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Items with Price</th>
                                <th>Restaurant Name</th>
                                <th>Customer Name</th>
                                <th>Customer PhoneNo</th>
                                <th>Customer Address</th>
                                <th> </th>
                                <th>Order Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <!-- ejs syntax -->
                            <% voData.forEach(function(dbVOData){ %>
                            <tr>
                                <td><%= dbVOData.Items_with_Price %></td>
                                <td><%= dbVOData.Restaurant_Name %></td>
                                <td><%= dbVOData.Customer_Name %></td>
                                <td><%= dbVOData.Customer_PhoneNo %></td>
                                <td><%= dbVOData.Customer_Address %></td>
                                <td><%= dbVOData.Order_Total %></td>
                                <td>
                                    <form action="/oDelivered/<%= dbVOData.id %>" method="post">
                                        <button type="submit"  class="btn btn-success">Delivered</button>
                                        <input type="hidden" name="_csrf" value="<%=_csrf%>">
                                    </form>
                                </td>
                            </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
    <!-- Shoping Cart Section End -->